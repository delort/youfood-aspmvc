@model IEnumerable<tmf.entities.OrderPaid>

@{
    ViewBag.Title = "Order Paid";
}

<h2><span class="titleContent">Order Paid</span></h2>

<div class="bodyContent">
<table id="ordersPaid">
    <tr>
        <th></th>
        <th>
            Table
        </th>
        <th>
            Waiter
        </th>
        <th>
            Restaurant
        </th>
        <th>
            Menus
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        @Html.Partial("Order", item)
    </tr>
}

</table>
    </div>
<script type="text/javascript">
    connectHub("paid", "ordersPaid");
</script>
